<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root spacing="7.0" type="VBox" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="name" />
      <Label fx:id="money" text="Money:   " />
      <Label fx:id="profit" text="Profit: " />
      <Label fx:id="hasButton" text="Has button" visible="false" />
      <Label fx:id="folded" text="Folded" visible="false">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Label>
   </children>
   <padding>
      <Insets bottom="7.0" left="7.0" right="7.0" top="7.0" />
   </padding>
</fx:root>
