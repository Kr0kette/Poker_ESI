<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>


<fx:root type="VBox" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="addPlayerButton" mnemonicParsing="false" onAction="#addPlayer" text="addPlayer" />
      <TextField fx:id="addPlayerName" promptText="Player Name" />
      <TextField fx:id="addPlayerMoney" promptText="Player Money" />
      <Button fx:id="startButton" mnemonicParsing="false" onAction="#start" text="Start" visible="false" />
   </children>
</fx:root>
